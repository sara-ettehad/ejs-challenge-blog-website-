<%- include("partials/header"); -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label >Title</label>
        <input type="text" name="postTitle" class="form-control">
    </div>
    <div class="form-group">
        <label >Post</label>
        <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">publish</button>
</form>

