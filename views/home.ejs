<%- include("partials/header"); -%>

<h1>Home</h1>

<p> <%= StartingContent %> </p>

<% posts.forEach(function(post){ %>
    <h1> <%= post.Title %> </h1>
    <p> 
        <%= post.Content.substring(0, 100) + "..." %> 
        <a href="/posts/<%= post.Title %>">Read More</a>
    </p>

<% }) %>

<%- include("partials/footer"); -%>